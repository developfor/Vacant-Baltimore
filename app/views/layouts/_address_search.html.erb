<%= form_for( VacantBuilding.new, :url => "search/address_search",:method => :get, :remote => true) do |f| %>
  <%#= f.label :searchLocation %>
  <%= f.text_field :searchLocation, value: "",  size: 25, :placeholder => "Enter Address" %>
 
  <%= f.submit "Go" , id: "submitter" %>
<% end %>

<script type="text/javascript">

$("#submitter").on("click", function(){
	console.log($('#vacant_building_searchLocation').serialize())
	
	$.getJSON('http://localhost:3000/search/address_search?utf8=%E2%9C%93&'+$('#vacant_building_searchLocation').serialize()+'&commit=Find+Details', function(result) {

		
		
		map.panTo(new L.LatLng(result["lat"], result["lng"])).zoomIn(18);
		map.fire('dragend');

		// console.log(<%= @get_address.to_s %>);

	});

}); 


</script>